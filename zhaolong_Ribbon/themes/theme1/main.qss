/*================
统一：
color:#424242;
background-color: #FAFAFA;
normal widget border-color: C0C0C0;
edit border-color: AFAFAA;

button background-color:FAFAFA
button pressed background-color:FDEDA3
button focus background-color:FAFAFA
button border:AAAAAA;
button disabled border:AAAAAA;
button disabled background-color:DDDDDD
button hover background-color:#FDECAD
selection-color: #545454;
selection-background-color:#FDD36A;

disabled color: #76797C;
disabled background-color: #FAFAAA;

item:hover background-color: #FDD36A;
item:hover color: #424242;

alternate-background-color: #FFF;

focus border #6D6D6D;
==================*/

QComboBox:hover
,QAbstractSpinBox:hover
,QLineEdit:hover
,QCheckBox:hover
,QTextEdit:hover
,QPlainTextEdit:hover
,QAbstractView:hover
,QTreeView:hover
{
    border: 1px solid #6D6D6D;
}

/*================
QAbstractSpinBox
==================*/
QAbstractSpinBox {
    padding: 2px 5px 2px 5px;
    border: 1px solid #AFAFAA;
    border-radius: 2px;
}
QAbstractSpinBox:up-button
{
    background-color: transparent;
    subcontrol-origin: border;
    subcontrol-position: center right;
    height: 100%;
    border-left: 1px solid #AFAFAA;
    border-top:none;
    border-right: none;
    border-bottom: none;
    padding-left: 2px;
    padding-right: 2px;
    width: 15px;
}

QAbstractSpinBox:down-button
{
    background-color: transparent;
    subcontrol-origin: border;
    subcontrol-position: center left;
    height: 100%;
    border-left: none;
    border-top:none;
    border-right: 1px solid #AFAFAA;
    border-bottom: none;
    padding-left: 2px;
    padding-right: 2px;
    width: 15px;
}

QAbstractSpinBox::up-arrow,QAbstractSpinBox::up-arrow:disabled,QAbstractSpinBox::up-arrow:off {
    image: url(./theme/theme1/icon/add.png);
    width: 10px;
    height: 10px;
}
QAbstractSpinBox::up-arrow:hover
{
    image: url(./theme/theme1/icon/add-hover.png);
    
}
QAbstractSpinBox::down-arrow,QAbstractSpinBox::down-arrow:disabled,QAbstractSpinBox::down-arrow:off
{
    image: url(./theme/theme1/icon/sub.png);
    width: 10px;
    height: 10px;
}
QAbstractSpinBox::down-arrow:hover
{
    image: url(./theme/theme1/icon/sub-hover.png);
}
/*================
QWidget
==================*/
QWidget
{
    color: #424242;
    background-color: #FAFAFA;
    selection-background-color:#FDD36A;
    selection-color: #000;
    background-clip: border;
    border-image: none;
    outline: 0;
}
QWidget:item:hover
{
    background-color: #FDD36A;
    color: #000;
}
QWidget:item:selected
{
    background-color: #FDD36A;
    color: #000;
}

QWidget:disabled
{
    color: #76797C;
    background-color: #FAFAFA;
}

QWidget:focus
,QMenuBar:focus
{
    border: 1px solid #6D6D6D;
}
QCheckBox:focus
,QRadioButton:focus
,QTabWidget:focus
,QSlider:focus
{
border: none;
}

/*================
QDialog
==================*/
QDialog
{
    color: #424242;
    background-color: #FAFAFA;
    selection-background-color:#FDD36A;
    selection-color: #000;
    background-clip: border;
    border-image: none;
    outline: 0;
}
QDialog:item:hover
{
    background-color: #FDD36A;
    color: #000;
}
QDialog:item:selected
{
    background-color: #FDD36A;
    color: #FFF;
}

QDialog:disabled
{
    color: #76797C;
    background-color: #FAFAFA;
}

QDialog:focus
,QMenuBar:focus
{
    border: 1px solid #6D6D6D;
}
QCheckBox:focus
,QRadioButton:focus
,QTabWidget:focus
,QSlider:focus
{
border: none;
}

/*================
QFrame
==================*/
QFrame
{
    border-radius: 2px;
    border: 1px solid #C0C0C0;
}

QFrame[frameShape="0"]
{
    border-radius: 2px;
    border: 1px transparent #C0C0C0;
}
/*================
QToolTip
==================*/

QToolTip
{
    border: 1px solid #76797C;
    background-color: #FAFAFA;
    color: #424242;
    padding: 5px;
    opacity: 200;
}

/*================
QCheckBox QGroupBox-QCheckBox
==================*/
QCheckBox
{
    spacing: 10px;
    outline: none;
    color: #424242;
    margin-bottom: 2px;
}

QCheckBox:disabled
{
    color: #76797C;
}

QCheckBox::indicator,
QGroupBox::indicator
{
    width: 18px;
    height: 18px;
}
QGroupBox::indicator
{
    margin-left: 2px;
}


