/*================
QPushButton
==================*/
QAbstractButton{
    border-radius: 2px;
}
QPushButton
{
    color: #424242;
    background-color: #FAFAFA;
    border: 1px solid #AAAAAA;
    padding-top: 5px;
    padding-bottom: 5px;
    padding-left: 10px;
    padding-right: 10px;
    border-radius: 2px;
    outline: none;
}
QPushButton:disabled
{
    background-color: #DDDDDD;
    border: 1px solid #AAAAAA;
    padding-top: 5px;
    padding-bottom: 5px;
    padding-left: 10px;
    padding-right: 10px;
    border-radius: 0px;
    color: #76797C;
}
QPushButton:pressed
{
    background-color: #FDEDA3;
    border: 1px solid #F2CA58;
    color: 424242;
}
QPushButton:focus {
    background-color: #DEDEDE;
    border: 1px solid #AFAFAA;
    color: 424242;
}
QPushButton:checked{
    background-color: #9ad4bd;
    border: 1px solid #249B6D;
}

QPushButton:hover
{
    background-color:#DEDEDE;
    border: 1px solid #000;
    color: #000;
}
/*================
QToolButton
==================*/
QToolButton {
    background-color: #FAFAFA;
    border: none;
}
QToolButton:hover, QToolButton::menu-button:hover {
    background-color: #DEDEDE;
    border: 1px solid #AFAFAA;
    padding: 2px;
}
QToolButton:checked, QToolButton:pressed,
        QToolButton::menu-button:pressed {
    background-color: #cfcfcf;
    border: 1px solid #AFAFAA;
    padding: 2px;
}
/*================
QComboBox
==================*/
QComboBox
{
    selection-background-color: #FDD36A;
    border: 1px solid #AFAFAA;
    border-radius: 2px;
    padding: 2px;
    min-width: 50px;
}
QComboBox:on
{
    padding-top: 2px;
    padding-left: 4px;
}
QComboBox QAbstractItemView
{
    background-color: #FAFAFA;
    border-radius: 2px;
    border: 1px solid #AFAFAA;
    selection-background-color: #FDD36A;
}

QComboBox::drop-down
{
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 15px;

    border-left-width: 0px;
    border-left-color: #ff0;
    border-left-style: solid;
    border-top-right-radius: 3px;
    border-bottom-right-radius: 3px;
}

QComboBox::down-arrow
{
    image: url(./theme/theme1/icon/ArrowDown.png);
}
QComboBox::down-arrow:hover
{
    image: url(./theme/theme1/icon/ArrowDown-hover.png);
}

/*================
QLineEdit
==================*/
QLineEdit
,QTextEdit
,QPlainTextEdit
{
    padding: 1px;
    border-style: solid;
    border: 1px solid #AFAFAA;
    border-radius: 3px;
    color: #424242;
}

/*================
QGroupBox
==================*/
QGroupBox {
    border:1px solid #AFAFAA;
    border-radius: 2px;
    margin-top: 20px;
}

QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top left;
    padding-left: 10px;
    padding-right: 10px;
    padding-top: 10px;
}

/*================
QAbstractScrollArea
==================*/
QAbstractScrollArea
{
    border-radius: 2px;
    border: 1px solid #76797C;
    background-color: transparent;
}
/*================
QMainWindow
==================*/
QMainWindow::separator
{
    background-color: #E3E6E8;
    color: white;
    padding-left: 3px;
    spacing: 2px;
    border: none;
}
QMainWindow::separator:hover
{

    background-color: #C4C6C6;
    color: white;
    padding-left: 3px;
    border: none;
    spacing: 2px;
}
/*================
QHeaderView
==================*/
QHeaderView
{
    background-color: #F0F2F2;
    border: 1px solid #E3E6E8;
    border-radius: 0px;
    margin: 0px;
    padding: 0px;
}
QHeaderView::section
{
    background-color: #F0F2F2;
    color: #424242;
    padding: 2px;

    border-right: 1px solid #AFAFAA;
    border-bottom: 1px solid #AFAFAA;
    border-top: none;
    border-left: none;

}

QHeaderView::section:hover
{
    background-color: #FDD36A;
    color: #000;
    padding: 1px;
    border-right: 1px solid #AFAFAA;
    border-bottom: 1px solid #AFAFAA;
    border-top: none;
    border-left: none;
}
QHeaderView::section:checked
 {
    color: #000;
    background-color: #D3D6D8;
 }

 QHeaderView::section::vertical::first, QHeaderView::section::vertical::only-one
 {
     border-top: 1px solid #AFAFAA;
 }
 QHeaderView::section::horizontal::first, QHeaderView::section::horizontal::only-one
 {
     border-left: 1px solid #AFAFAA;
 }
 QHeaderView::down-arrow {
     image: url(./theme/theme1/icon/ArrowDown.png);
 }

 QHeaderView::up-arrow {
     image: url(./theme/theme1/icon/ArrowUp.png);
 }

/*================
QRadioButton
==================*/

QRadioButton
{
    spacing: 5px;
    outline: none;
    color: #424242;
    margin-bottom: 2px;
}
QRadioButton:disabled
{
    color: #76797C;
}
QRadioButton::indicator
{
    width: 16px;
    height: 16px;
}

QRadioButton::indicator:unchecked
{
    image: url(./theme/theme1/icon/radio.png);
}
QRadioButton::indicator:unchecked:hover,
QRadioButton::indicator:unchecked:focus,
QRadioButton::indicator:unchecked:pressed
{
    border: none;
    outline: none;
    image: url(./theme/theme1/icon/radio-hover.png);
}
QRadioButton::indicator:checked
{
    border: none;
    outline: none;
    image: url(./theme/theme1/icon/radio-on.png);
}
QRadioButton::indicator:checked:hover,
QRadioButton::indicator:checked:focus,
QRadioButton::indicator:checked:pressed
{
    border: none;
    outline: none;
    image: url(./theme/theme1/icon/radio-on-hover.png);
}
QRadioButton::indicator:checked:disabled
{
    outline: none;
    image: url(./theme/theme1/icon/radio-on-disable.png);
}

QRadioButton::indicator:unchecked:disabled
{
    image: url(./theme/theme1/icon/radio-disable.png);
}


/*================
QMenu
==================*/
QMenu{
    border: 1px solid #76797C;
    color: #424242;
    margin: 2px;
}
QMenu::icon
{
    margin: 5px;
}
QMenu::item
{
    padding: 5px 30px 5px 30px;
    margin-left: 5px;
    border: 1px solid transparent;
}
QMenu::item:selected
{
    color: #000;
}
QMenu::separator {
    height: 1px;
    background: #76797C;
    margin-left: 10px;
    margin-right: 5px;
}
QMenu::indicator {
    width: 18px;
    height: 18px;
}

