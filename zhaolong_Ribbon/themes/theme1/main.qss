/*================
QAbstractItemView
==================*/

QAbstractItemView
{
    alternate-background-color: #DEDEDE;
    color: #424242;
    border: 1px solid #C0C0C0;
    border-radius: 2px;
}

/*================
QDockWidget
==================*/
QDockWidget
{
    border: 1px solid lightgray;
}

QDockWidget::title
{
  text-align: left;
  background: #c7cfd1;
  padding-left: 5px;
}

QDockWidget::title:focus
{
  text-align: left;
  background: #3DAEE9;
  color:#FFFFFF;
  padding-left: 5px;
}
/*================
QTabBar
=================*/
QTabWidget{
    border: 0px transparent black;
}
QTabWidget::pane
{
    border-top: 0px solid #9D9D9D;
    padding: 0px;
    margin: 0px;
    top: -1px;
    bottom: -1px;
    left: -1px;
    right: -1px;
}

QTabBar
{
    qproperty-drawBase: 0;
    left: 5px;
    border-radius: 3px;
    background-color: transparent;
}
QTabBar:focus
{
    border: 0px transparent black;
}

QTabBar::tab:top{
    color: #1A1A1A;
    border-top: 1px solid #9D9D9D;
    border-right: 1px solid #9D9D9D;
    border-left: 1px solid #9D9D9D;
    border-bottom: none;
    background-color: #FFFFFF;
    padding: 5px;
    /*min-width: 50px;*/
    border-top-left-radius: 5px;
    border-top-right-radius: 5px;
}
QTabBar::tab:top:!selected
{
    color: #1A1A1A;
    border-top: none;
    border-right: none;
    border-left: none;
    border-bottom: 1px solid #9D9D9D;
    background-color: #FFFFFF;
}
QTabBar::tab:top:!selected:hover {
    border-top: 1px solid #FFB739;
    border-right: 1px solid #FFB739;
    border-left: 1px solid #FFB739;
    border-bottom: none;
}


QTabBar::tab:bottom{
    color: #1A1A1A;
    border-top: none;
    border-right: 1px solid #9D9D9D;
    border-left: 1px solid #9D9D9D;
    border-bottom: 1px solid #9D9D9D;
    background-color: #FFFFFF;
    padding: 5px;
    /*min-width: 50px;*/
    border-bottom-left-radius: 5px;
    border-bottom-right-radius: 5px;
}
QTabBar::tab:bottom:!selected
{
    color: #1A1A1A;
    border-top: 1px solid #9D9D9D;
    border-right: none;
    border-left: none;
    border-bottom: none;
    background-color: #FFFFFF;
}
QTabBar::tab:bottom:!selected:hover {
    border-top: none;
    border-right: 1px solid #FFB739;
    border-left: 1px solid #FFB739;
    border-bottom: 1px solid #FFB739;
}

QTabBar::tab:left{
    color: #1A1A1A;
    border-top: 1px solid #9D9D9D;
    border-right: none;
    border-left: 1px solid #9D9D9D;
    border-bottom: 1px solid #9D9D9D;
    background-color: #FFFFFF;
    padding: 5px;
    /*min-width: 50px;*/
    border-bottom-left-radius: 5px;
    border-top-left-radius: 5px;
}
QTabBar::tab:left:!selected
{
    color: #1A1A1A;
    border-top: none;
    border-right: none;
    border-left: 1px solid #9D9D9D;
    border-bottom: none;
    background-color: #FFFFFF;
}
QTabBar::tab:left:!selected:hover {
    border-top: 1px solid #FFB739;
    border-right: none;
    border-left: 1px solid #FFB739;
    border-bottom: 1px solid #FFB739;
}

QTabBar::tab:right{
    color: #1A1A1A;
    border-top: 1px solid #9D9D9D;
    border-right: 1px solid #9D9D9D;
    border-left: none;
    border-bottom: 1px solid #9D9D9D;
    background-color: #FFFFFF;
    padding: 5px;
    /*min-width: 50px;*/
    border-bottom-left-radius: 5px;
    border-top-left-radius: 5px;
}
QTabBar::tab:right:!selected
{
    color: #1A1A1A;
    border-top: none;
    border-right: 1px solid #9D9D9D;
    border-left: none;
    border-bottom: none;
    background-color: #FFFFFF;
}
QTabBar::tab:right:!selected:hover {
    border-top: 1px solid #FFB739;
    border-right: 1px solid #FFB739;
    border-left: none;
    border-bottom: 1px solid #FFB739;
}
/*================
QScrollBar
=================*/

QScrollBar:horizontal
{
    margin: 2px 15px 2px 15px;
    border: none;
    border-radius: 4px;
    background-color: #AAABB3;
    height: 16px;
}
QScrollBar::handle:horizontal
{
    background-color: #AFAFAA;
    min-width: 5px;
    border-radius: 4px;
    height: 10px;
}
QScrollBar::handle:horizontal:hover
{
    margin: 1px 0px 1px 0px;
    background-color: #E8E8E8;
    min-width: 5px;
    border-radius: 4px;
    height: 10px;
}

QScrollBar::add-line:horizontal
{
    margin: 0px 3px 0px 3px;
    border-image: url(./theme/theme1/icon/ArrowRight.png);
    width: 12px;
    height: 12px;
    subcontrol-position: right;
    subcontrol-origin: margin;
    background-color: #AAABB3;
    border:none;
}


QScrollBar::sub-line:horizontal
{
    margin: 0px 3px 0px 3px;
    border-image: url(./theme/theme1/icon/ArrowLeft.png);
    height: 12px;
    width: 12px;
    subcontrol-position: left;
    subcontrol-origin: margin;
    background-color: #AAABB3;
    border:none;
}

QScrollBar::add-line:horizontal:hover,QScrollBar::add-line:horizontal:on
{
    border-image: url(./theme/theme1/icon/ArrowRight-hover.png);
    background-color: #E8E8E8;
    border:1px solid #000;
}

QScrollBar::sub-line:horizontal:hover, QScrollBar::sub-line:horizontal:on
{
    border-image: url(./theme/theme1/icon/ArrowLeft-hover.png);
    background-color: #E8E8E8;
    border:1px solid #000;
}

QScrollBar:vertical
{
    margin: 15px 2px 15px 2px;
    border: none;
    border-radius: 4px;
    background-color: #AAABB3;
    width: 16px;
}

QScrollBar::handle:vertical
{
    margin: 1px 1px 1px 1px;
    background-color: #AFAFAA;
    min-height: 5px;
    border-radius: 4px;
}
QScrollBar::handle:vertical:hover
{
    margin: 1px 1px 1px 1px;
    background-color: #E8E8E8;
    min-height: 5px;
    border-radius: 4px;
}

QScrollBar::sub-line:vertical
{
    margin: 3px 0px 3px 0px;
    border-image: url(./theme/theme1/icon/ArrowUp.png);
    height: 12px;
    width: 12px;
    subcontrol-position: top;
    subcontrol-origin: margin;
    background-color: #AAABB3;
    border:none;
}

QScrollBar::add-line:vertical
{
    margin: 3px 0px 3px 0px;
    border-image: url(./theme/theme1/icon/ArrowDown.png);
    height: 12px;
    width: 12px;
    subcontrol-position: bottom;
    subcontrol-origin: margin;
    background-color: #AAABB3;
    border:none;
}

QScrollBar::sub-line:vertical:hover,QScrollBar::sub-line:vertical:on
{
    border-image: url(./theme/theme1/icon/ArrowUp-hover.png);
    background-color: #E8E8E8;
    border:1px solid #000;
}


QScrollBar::add-line:vertical:hover, QScrollBar::add-line:vertical:on
{
    border-image: url(./theme/theme1/icon/ArrowDown-hover.png);
    background-color: #E8E8E8;
    border:1px solid #000;
}
/*================
QStackedWidget
=================*/
QStackedWidget
{
    border: none;
}
/*================
QSlider
=================*/
QSlider::groove:horizontal {
    border: 1px solid #C0C0C0;
    height: 2px;
    background: #C0C0C0;
    margin: 0px;
    border-radius: 2px;
}

QSlider::handle:horizontal {
    background: #FAFAFA;
    border: 1px solid #AAAAAA;
    width: 16px;
    height: 16px;
    margin: -8px 0;
    border-radius: 9px;
}
QSlider::handle:horizontal:hover {
    background-color:#C0C0C0;
    border: 1px solid #AFAFAA;
}
QSlider::groove:vertical {
    border: 1px solid #C0C0C0;
    width: 2px;
    background: #C0C0C0;
    margin: 0px;
    border-radius: 3px;
}

QSlider::handle:vertical {
    background: #FAFAFA;
    border: 1px solid #AAAAAA;
    width: 16px;
    height: 16px;
    margin: 0 -8px;
    border-radius: 9px;
}
QSlider::handle:vertical:hover {
    background-color:#C0C0C0;
    border: 1px solid #AFAFAA;
}

/*================
QTableView
=================*/
QTableView
{
    border: 1px solid #C0C0C0;
    gridline-color: #C0C0C0;
    background-color: #FAFAFA;
}


QTableView, QHeaderView
{
    border-radius: 0px;
}

QTableView::item:pressed, QListView::item:pressed, QTreeView::item:pressed  {
    background-color: #3daee9;
    color: #FFF;
}

QTableView::item:selected:active, QTreeView::item:selected:active, QListView::item:selected:active  {
    background: #3daee9;
    color: #eff0f1;
}
/*================
QTableCornerButton
=================*/
QTableCornerButton {
    background-color: #F0F2F2;
    border: none;
}

QTableCornerButton::section {
    background-color: #F0F2F2;

}
